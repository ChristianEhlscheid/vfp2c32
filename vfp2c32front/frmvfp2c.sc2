	PROCEDURE addtypetosym
		LOCAL lnIndex, loType, xj
		lnIndex = THIS.PFMain.Page1.oList.ListIndex
		
		IF lnIndex = 0
			MESSAGEBOX("Select a type from the listbox first!",48,"VFP2C32")
			RETURN
		ENDIF
		
		loType = THIS.oParser.PrepareTypeForCG(lnIndex)
		IF VARTYPE(loType) = 'L'
			THIS.Refresh()
			RETURN
		ENDIF
		
		SELECT ctypedefs
		
		FOR xj = 1 TO ALEN(loType.laDeclar)
			LOCATE FOR ALLTRIM(typename) == loType.laDeclar[xj].cName
		
			IF FOUND()
				IF MESSAGEBOX("Type '" + loType.laDeclar[xj].cName + "' already in Symbol Table" + CRLF + ;
							"Overwrite?",36,"VFP2C32") = 7
					LOOP
				ENDIF
			ELSE
				APPEND BLANK
			ENDIF
		
			REPLACE typename WITH loType.laDeclar[xj].cName
			IF loType.Typemask = TM_STRUCT
				REPLACE vfpclass WITH loType.laDeclar[xj].cName, basetype WITH "struct"
			ELSE
				REPLACE vfpclass WITH "", basetype WITH "enum"
			ENDIF
			
			REPLACE indirect WITH loType.laDeclar[xj].Indirect
			REPLACE typemask WITH loType.Typemask, charset WITH 0, platform WITH 0
			IF loType.laDeclar[xj].Indirect >= 1
				REPLACE sizeof WITH THIS.oParser.nPointerSize, alignof WITH THIS.oParser.nPointerSize
			ELSE
				REPLACE sizeof WITH loType.laDeclar[xj].SizeOf, alignof WITH loType.laDeclar[xj].AlignOf
			ENDIF
			TABLEUPDATE(.T.,.T.,'ctypedefs')
		ENDFOR
		
		THIS.oParser.InitTypeList()
	ENDPROC

	PROCEDURE Destroy
		THIS.oParser = .NULL.
		THIS.oCodeGen = .NULL.
		
	ENDPROC

	PROCEDURE displayerrors
	ENDPROC

	PROCEDURE Init
		THIS.oParser.ClearErrors()
		
		LOCAL laType[1]
		SELECT typemask FROM ctypedefs WHERE ALLTRIM(typename) == 'BYTE' INTO ARRAY laType
		DO CASE
			CASE laType[1] = TM_CHAR+TM_UNSIGNED
				THIS.PFMain.Page1.pfOptions.Page1.optBYTE.Value = 1
			CASE laType[1] = TM_INT8+TM_UNSIGNED
				THIS.PFMain.Page1.pfOptions.Page1.optBYTE.Value = 2
			CASE laType[1] = TM_BYTE
				THIS.PFMain.Page1.pfOptions.Page1.optBYTE.Value = 3
		ENDCASE
		
		CREATE CURSOR callocators (cAllocator C(30), nAllocator I)
		INSERT INTO callocators VALUES ("FLL Allocator",1)
		INSERT INTO callocators VALUES ("Global Allocator (GlobalAlloc)",2)
		LOCATE
		
		CREATE CURSOR ccharsets (cCharset C(10), nCharset I)
		INSERT INTO ccharsets VALUES ("",0)
		INSERT INTO ccharsets VALUES ("Ansi",1)
		INSERT INTO ccharsets VALUES ("Unicode",2)
		
		CREATE CURSOR cindirection (cIndirect C(2), nIndirect I)
		INSERT INTO cindirection VALUES ("",0)
		INSERT INTO cindirection VALUES ("*",1)
		INSERT INTO cindirection VALUES ("**",2)
		
		THIS.BindControls = .T.
		
		THIS.SetCodeProperties(0)
		THIS.PFMain.Page1.pfInOut.Page1.edtInput.SetFocus()
		
		
		
		
		
	ENDPROC

