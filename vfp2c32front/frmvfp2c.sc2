	PROCEDURE setcodeproperties
		LPARAMETERS lnLanguage
		LOCAL loPage
		DO CASE
			CASE lnLanguage = 0 && VFP
				loPage = THIS.pfMain.Page1.pfOptions.Page1
				FOR EACH loControl IN loPage.Controls
					IF PEMSTATUS(loControl,'SetCodeProperty',5)
						loControl.SetCodeProperty(.T.)
					ENDIF
				ENDFOR
				loPage = THIS.pfMain.Page1.pfOptions.Page2
				FOR EACH loControl IN loPage.Controls
					IF PEMSTATUS(loControl,'SetCodeProperty',5)
						loControl.SetCodeProperty(.T.)
					ENDIF
				ENDFOR
			CASE lnLanguage = 1 && C#
				loPage = THIS.pfMain.Page1.pfOptions.Page3
				FOR EACH loControl IN loPage.Controls
					IF PEMSTATUS(loControl,'SetCodeProperty',5)
						loControl.SetCodeProperty(.T.)
					ENDIF
				ENDFOR
				loPage = THIS.pfMain.Page1.pfOptions.Page4
				FOR EACH loControl IN loPage.Controls
					IF PEMSTATUS(loControl,'SetCodeProperty',5)
						loControl.SetCodeProperty(.T.)
					ENDIF
				ENDFOR
		ENDCASE
			
			
	ENDPROC

