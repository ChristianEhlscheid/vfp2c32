	PROCEDURE startparse
		THIS.oParser.ClearErrors()
		
		WITH THIS.PFMain.Page1
			.cmdGenCode.Enabled = .F.
			.cmdGenTestCode.Enabled = .F.
			.cmdGenOffsetList.Enabled = .F.
			.cmdAddTypeToSym.Enabled = .F.
		ENDWITH
		
		THIS.oParser.ClearParsedTypes()
		THIS.PFMain.Page1.oList.Clear()
		
		STRTOFILE(THIS.PFMain.Page1.PFInOut.Page1.edtInput.Value,'input.c',0)
		
		THIS.oParser.RemoveComments('input.c','intermediate.c')
		
		THIS.PFMain.Page1.PFInOut.Page1.edtInput.Value = FILETOSTR('intermediate.c')
		
		IF THISFORM.oParser.Parsefile('intermediate.c') != 0
			THIS.oParser.DisplayErrors()
			THIS.Refresh()
			RETURN
		ENDIF
		
		IF THIS.oParser.nErrorCount > 0
			THIS.oParser.DisplayErrors()
			THIS.Refresh()
			RETURN
		ENDIF
		
		THISFORM.oParser.WalkParsedTypes()
		
		IF THISFORM.oParser.nWalkErrors > 0
			THIS.Refresh()
			RETURN
		ENDIF
		
		WITH THIS.PFMain.Page1
			.cmdGenCode.Enabled = .T.
			.cmdGenTestCode.Enabled = .T.
			.cmdGenOffsetList.Enabled = .T.
			.cmdAddTypeToSym.Enabled = .T.
		ENDWITH
		
		LOCAL xj
		
		WITH THIS.PFMain.Page1.oList
		
		FOR xj = 1 TO ALEN(THIS.oParser.laTypes)
			DO CASE
				CASE !EMPTY(THIS.oParser.laTypes[xj].cName)
					.AddItem(THIS.oParser.laTypes[xj].cName)
				CASE VARTYPE(THIS.oParser.laTypes[xj].laDeclar[1]) = 'O'
					.AddItem(THIS.oParser.laTypes[xj].laDeclar[1].cName)
				OTHERWISE
					.AddItem("Noname type")
			ENDCASE
		ENDFOR
		
		ENDWITH
		
		INSERT INTO cErrors VALUES (1,'Parsing successful!',.NULL.)
		
		THIS.Refresh()
		
		WITH THIS.PfMain.Page1.olist
		.SetFocus()
		.ListIndex = 1
		ENDWITH
		
		
		
	ENDPROC

	PROCEDURE Unload
		USE IN SELECT('ccharsets')
		USE IN SELECT('cindirection')
		
		IF _VFP.StartMode != 0
			CLEAR EVENTS
			SET LIBRARY TO
			RELEASE PROCEDURE cparser, codegen, cparsetypes
		ENDIF
		
	ENDPROC

