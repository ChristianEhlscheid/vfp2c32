	PROCEDURE PFMain.Page1.pfOptions.Page1.optEmbedding.Init
		LOCAL lcText
		
		lcText = "This option has effect on the memory allocation/cleanup code that is produced for the struct." + CRLF + CRLF
		lcText = lcText + "responsible:" + CRLF + "the struct is responsible to allocate and free it's own memory" + CRLF + CRLF
		lcText = lcText + "not responsible:" + CRLF + "the struct is not responsible to do any memory operations, instead a pointer to it's memory location is passed to it" + CRLF + CRLF
		lcText = lcText + "context dependent:" + CRLF + "the struct decides at runtime if it needs to manage memory or not depending on it's initialization parameters"
		
		THIS.ToolTipText = lcText
		THIS.optbbase1.ToolTipText = lcText
		THIS.optbbase2.ToolTipText = lcText
		THIS.optbbase3.ToolTipText = lcText
